<script>
  const quantityInputs = document.querySelectorAll('.quantity_input');
  const subtotalCells = document.querySelectorAll('.subtotal');
  const totalCell = document.querySelector('#total');
  
  quantityInputs.forEach((input, index) => {
    input.addEventListener('change', () => {
      const quantity = parseInt(input.value);
      const price = parseFloat(input.closest('tr').querySelector('.product_price').textContent);
      const subtotal = quantity * price;
      subtotalCells[index].textContent = subtotal.toFixed(2);
      
      let total = 0;
      subtotalCells.forEach(cell => {
        total += parseFloat(cell.textContent);
      });
      totalCell.textContent = total.toFixed(3) + "  TND";
    });
  });
</script>
